{
  "name": "ember-toucan-core-monorepo",
  "version": "0.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:CrowdStrike/ember-toucan-core.git",
    "directory": "ember-toucan-core"
  },
  "license": "MIT",
  "author": "CrowdStrike UX Team",
  "scripts": {
    "prepare": "pnpm build",
    "build": "pnpm --filter ember-toucan-core build",
    "start": "concurrently 'npm:start:*' --restart-after 5000 --prefix-colors cyan,white,yellow",
    "start:tests": "pnpm --filter test-app start",
    "start:addon": "pnpm --filter ember-toucan-core start --no-watch.clearScreen",
    "test": "pnpm --filter test-app test",
    "lint": "pnpm --filter '*' lint",
    "lint:fix": "pnpm --filter '*' lint:fix"
  },
  "devDependencies": {
    "concurrently": "^7.2.1",
    "prettier": "^2.5.1"
  },
  "workspaces": [
    "ember-toucan-core",
    "test-app"
  ]
}