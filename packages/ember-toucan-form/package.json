    "tailwindcss": "^2.2.15",
    "typescript": "^5.0.0"
  },
  "dependenciesMeta": {
    "@crowdstrike/ember-toucan-core": {
      "injected": true
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "version": 2,
    "type": "addon",
    "main": "addon-main.cjs",
    "app-js": {
      "./components/toucan-form.js": "./dist/_app_/components/toucan-form.js"
    }
  },
  "exports": {
    ".": "./dist/index.js",
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    },
    "./addon-main.js": "./addon-main.cjs"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  }
